! URXVT
URxvt.perl-lib: /home/andrew/.config/urxvt/ext
URxvt.font: xft:DejaVu Sans Mono:style=Book:size=9, xft:DejaVuSansMono Nerd Font Mono, xft:Noto Sans Mono CJK JP, xft:Noto Sans Mono CJK TC, xft:Noto Sans Mono CJK KR, xft:Noto Sans Mono CJK HK
Urxvt.boldFont: xft:DejaVuSansMono Nerd Font Mono:style=Bold:size=9
Urxvt.italicFont: xft:DejaVuSansMono Nerd Font Mono:style=Oblique:size=9
Urxvt.boldItalicFont: xft:DejaVuSansMono Nerd Font Mono:style=Bold Oblique:size=9

! URxvt.font: xft:FuraCode Nerd Font:style=Regular:size=9, xft:Noto Sans Mono CJK JP, xft:Noto Sans Mono CJK TC, xft:Noto Sans Mono CJK KR, xft:Noto Sans Mono CJK HK
! URxvt.boldFont: xft:FuraCode Nerd Font:style=Bold:size=9
! Urxvt.italicFont: xft:FuraCode Nerd Font Mono:size=9
! Urxvt.boldItalicFont: xft:FuraCode Nerd Font Mono:size=9

URxvt.saveLines: 4096
URxvt.letterSpace: -1
! URxvt.lineSpace: -1
URxvt.scrollBar: false
URxvt.cursorBlink: false
URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0

URxvt.iso14755: false
URxvt.iso14755_52: false

! URxvt.perl-ext-common: default,matcher,resize-font
URxvt.perl-ext-common: default,resize-font,url-select,keyboard-select
URxvt.keysym.C-C: eval:selection_to_clipboard
URxvt.keysym.C-V: eval:paste_clipboard

URxvt.keysym.C-underscore: resize-font:smaller
URxvt.keysym.C-plus: resize-font:bigger
URxvt.keysym.C-parenright: resize-font:reset
URxvt.keysym.C-question: resize-font:show

URxvt.keysym.C-U: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
URxvt.url-select.button: 1

URxvt.keysym.C-S: perl:keyboard-select:activate

! URxvt.url-launcher: /usr/bin/xdg-open
! Urxvt.keysym.C-U: matcher:last
! Urxvt.keysym.C-I: matcher:list
! Urxvt.keysym.C-O: matcher:select
! URxvt.url-launcher: /usr/bin/firefox -private-window 
! URxvt.matcher.button: 1
! URxvt.matcher.rend.0: Uline Bold fg6
